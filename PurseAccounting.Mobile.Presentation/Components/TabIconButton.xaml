<?xml version="1.0" encoding="utf-8" ?>
<ContentView xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="PurseAccountinng.Mobile.Presentation.Components.TabIconButton">

    <ImageButton
        x:Name="ImageButton"
        WidthRequest="{StaticResource IconSize}"
        HeightRequest="{StaticResource IconSize}"
        MinimumWidthRequest="{StaticResource IconSize}"
        MinimumHeightRequest="{StaticResource IconSize}"
        Padding="0"
        BackgroundColor="Transparent"
        BorderColor="Transparent"
        Clicked="OnImageButtonClicked" />
    
</ContentView>
